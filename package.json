{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "server": "node index.js",
    "server:watch": "nodemon",
    "client": "ts-node ./fusebox.ts",
    "client:watch": "ts-node ./fusebox.ts --watch --hotModuleLoading --devServer",
    "prestart:prod": "tsc",
    "start:prod": "node dist/server.js",
    "start": "npm run server:watch & npm run client:watch",
    "lint": "tslint --project ./ --type-check",
    "lint:fix": "tslint --project ./ --type-check --fix"
  },
