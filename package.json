    "@types/axios": "^0.14.0",
    "@types/express": "^4.0.37",
    "@types/react": "^16.0.10",
    "@types/react-css-modules": "^4.2.1",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.10",
    "@types/redux-thunk": "^2.1.0",
    "@types/serve-index": "^1.7.28",
    "axios": "^0.16.2",
    "css-loader": "^0.28.7",
    "express": "^4.16.1",
